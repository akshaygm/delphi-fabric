{
  "TLS": true,
  "docker": {
    "fabricTag": "1.1.0",
    "thirdPartyTag": "0.4.6",
    "network": "delphiNetwork",
    "volumes": {
      "CONFIGTX": {
        "dir": "Documents/delphi-fabric/config/configtx"
      },
      "MSPROOT": {
        "dir": "Documents/delphi-fabric/config/ca-crypto-config/"
      }
    }
  },
  "orderer": {
    "type": "kafka",
    "genesis_block": {
      "file": "delphi.block",
      "profile": "delphiGenesis"
    },
    "solo": {
      "orgName": "Delphi.com",
      "container_name": "orderContainerName",
      "portHost": 7050,
      "ca": {
        "portHost": 7454
      },
      "MSP": {
        "name": "OrdererMSPName",
        "id": "OrdererMSP"
      }
    },
    "kafka": {
      "M": 2,
      "N": 3,
      "rule": "1<M<N<K,K>=4,Orderer.AbsoluteMaxBytes < replica.fetch.max.bytes <= message.max.bytes",
      "zookeepers": {
        "zookeeper0": {
          "MY_ID": 0
        },
        "zookeeper1": {
          "MY_ID": 1
        },
        "zookeeper2": {
          "MY_ID": 2
        }
      },
      "kafkas": {
        "kafka0": {
          "BROKER_ID": 0
        },
        "kafka1": {
          "BROKER_ID": 1
        },
        "kafka2": {
          "BROKER_ID": 2
        },
        "kafka3": {
          "BROKER_ID": 3
        }
      },
      "orgs": {
        "DelphiConsensus.Delphi.com": {
          "MSP": {
            "name": "DelphiMSPName",
            "id": "DelphiConsensusMSP"
          },
          "ca": {
            "portHost": 7454
          },
          "orderers": {
            "orderer0": {
              "portHost": 7050
            },
            "orderer1": {
              "portHost": 8050
            }
          }
        }
      }
    }
  },
  "channels": {
    "allchannel": {
      "file": "all.tx",
      "eventWaitTime": 30000,
      "orgs": {
        "ASTRI.Delphi.com": {
          "peerIndexes": [
            0
          ]
        }
      }
    }
  },
  "orgs": {
    "ASTRI.Delphi.com": {
      "peers": [
        {
          "container_name": "astri",
          "portMap": [
            {
              "host": 7051,
              "container": 7051
            },
            {
              "host": 7053,
              "container": 7053
            }
          ]
        }
      ],
      "ca": {
        "portHost": 7054
      },
      "MSP": {
        "name": "ASTRIMSPName",
        "id": "ASTRIMSP"
      }
    }
  }
}
