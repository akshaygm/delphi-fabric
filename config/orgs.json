{
  "TLS": false,
  "docker": {
    "fabricTag": "1.2.0",
    "thirdPartyTag": "0.4.10",
    "network": "delphiNetwork",
    "volumes": {
      "CONFIGTX": {
        "dir": "Documents/delphi-fabric/config/configtx"
      },
      "MSPROOT": {
        "dir": "Documents/delphi-fabric/config/ca-crypto-config/"
      }
    }
  },
  "ledger": {
    "storage": "leveldb",
    "couchDB": {
      "container_name": "couchDB",
      "port": 5984
    }
  },
  "orderer": {
    "type": "solo",
    "genesis_block": {
      "file": "delphi.block",
      "profile": "delphiGenesis"
    },
    "solo": {
      "orgName": "Delphi.com",
      "container_name": "orderer",
      "portHost": 7050,
      "ca": {
        "portHost": 7454
      },
      "MSP": {
        "name": "OrdererMSPName",
        "id": "OrdererMSP"
      }
    },
    "kafka": {
      "M": 2,
      "N": 3,
      "zookeepers": {
        "zookeeper0": {
          "MY_ID": 0
        },
        "zookeeper1": {
          "MY_ID": 1
        },
        "zookeeper2": {
          "MY_ID": 2
        }
      },
      "kafkas": {
        "kafka0": {
          "BROKER_ID": 0
        },
        "kafka1": {
          "BROKER_ID": 1
        },
        "kafka2": {
          "BROKER_ID": 2
        },
        "kafka3": {
          "BROKER_ID": 3
        }
      },
      "orgs": {
        "ICDD.ASTRI.org": {
          "MSP": {
            "name": "ICDDMSPName",
            "id": "ICDDMSP"
          },
          "ca": {
            "portHost": 8454
          },
          "orderers": {
            "orderer0": {
              "portHost": 8050
            }
          }
        }
      }
    }
  },
  "channels": {
    "allchannel": {
      "file": "all.tx",
      "eventWaitTime": 30000,
      "orgs": {
        "ASTRI.org": {
          "peerIndexes": [
            0,1
          ]
        }
      }
    }
  },
  "orgs": {
    "ASTRI.org": {
      "peers": [
        {
          "container_name": "peer0.ASTRI.org",
          "port": 7051
        },
        {
          "container_name": "peer1.ASTRI.org",
          "port": 7151
        }
      ],
      "ca": {
        "portHost": 7054
      },
      "MSP": {
        "name": "ASTRIMSPName",
        "id": "ASTRIMSP"
      }
    }
  }
}
